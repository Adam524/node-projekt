extends layout

block content
  h1= title
  p Welcome to #{title}
 
  form(method="GET")
    input(type='text' name="search" value=search)
    input(type="submit" value="Szukaj")

  each item in data
    article 
      h1(class="title")= item.title
      p(class="date")= item.date
      p(class="description")= item.description